  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{vue,ts}": [
      "eslint . --ext .vue,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
    ],
    "*.scss": [
      "npm run prettier"
    ]
  },
  "optionalDependencies": {
    "@types/ws": "^8.5.4",
    "cypress": "^12.10.0",
    "ts-node": "^10.9.1",
    "ws": "^8.13.0"
